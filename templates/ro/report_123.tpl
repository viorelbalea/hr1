{if empty($smarty.get.export) && empty($smarty.get.export_doc)}    <table cellspacing="0" cellpadding="2">        <tr>            <td>{translate label='An'}:</td>            <td>                <select name="Year" id="Year">                    <option value="0">{translate label='alege...'}</option>                    {foreach from=$years key=key item=item}                        <option value="{$item}" {if $smarty.get.Year == $item || (empty($smarty.get.Year) && $smarty.now|date_format:'%Y' == $item)}selected{/if}>{$item}</option>                    {/foreach}                </select>            </td>            <td>{translate label='Luna'}:</td>            <td>                <select name="Month" id="Month">                    <option value="0">{translate label='alege...'}</option>                    {foreach from=$months key=key item=item}                        <option value="{$item}" {if $smarty.get.Month == $item || (empty($smarty.get.Month) && $smarty.now|date_format:'%m' == $item)}selected{/if}>{$item}</option>                    {/foreach}                </select>            </td>            {if $smarty.session.USER_ID==1 || !empty($smarty.session.USER_COMPANYSELF)}                <td style="padding-left: 2px;" width="75">                    <select id="CompanyID" name="CompanyID" class="dropdown">                        <option value="0">{translate label='Companie self'}</option>                        {foreach from=$self key=key item=item}                            {if $smarty.session.USER_ID==1 || in_array($key, $smarty.session.USER_COMPANYSELF)}                                <option value="{$key}" {if $key==$smarty.get.CompanyID}selected{/if}>{$item}</option>                            {/if}                        {/foreach}                    </select>                </td>                <td style="padding-left: 2px;" width="75">                    <select id="DepartmentID" name="DepartmentID" class="dropdown">                        <option value="0">{translate label='Departament'}</option>                        {foreach from=$departments key=key item=item}                            <option value="{$key}" {if $key==$smarty.get.DepartmentID}selected{/if}>{$item}</option>                        {/foreach}                    </select>                </td>            {/if}            <td style="padding-left: 2px;" width="75">                <select id="SalaryPaymentType" name="SalaryPaymentType" class="dropdown">                    <option value="0">{translate label='Tip plata'}</option>                    {foreach from=$salary_payment_types key=key item=item}                        <option value="{$key}" {if $key==$smarty.get.SalaryPaymentType}selected{/if}>{translate label=$item}</option>                    {/foreach}                </select>            </td>            <td>&nbsp;</td>            <td><input type="button" value="{translate label='Afiseaza'}"                       onclick="window.location.href = './?m=reports&rep=123&GroupID={$smarty.get.GroupID}&CompanyID=' + document.getElementById('CompanyID').value + '&Year=' + document.getElementById('Year').value + '&Month=' + document.getElementById('Month').value + '&SalaryPaymentType=' + document.getElementById('SalaryPaymentType').value + '&DepartmentID='+document.getElementById('DepartmentID').value">            </td>            {if count($persons) > 0}                <td><input type="button" onclick="window.location.href = '{$smarty.server.REQUEST_URI}&export_txt=1'" value="Export .txt"></td>            {/if}        </tr>    </table>    <br>{/if}<table width="100%" cellspacing="0" cellpadding="2" border="1">    <tr>        {if empty($smarty.get.export)}            <td align="center">#</span></td>        {/if}        <td align="center">{translate label='Cont sursa'}</span></td>        <td align="center">{translate label='Cont destinatie'}</span></td>        <td align="center">{translate label='Suma'}</span></td>        <td align="center">{translate label='Beneficiar'}</span></td>        <td align="center">{translate label='Detalii 1'}</span></td>        <td align="center">{translate label='Detalii 2'}</span></td>    </tr>    {if count($persons) > 0}        {foreach from=$persons key=PersonID item=item name=persfor}            <tr>                {if empty($smarty.get.export)}                    <td align="center">{$smarty.foreach.persfor.iteration}</span></td>{/if}                <td align="center">RO71INGB5552999900780206</span></td>                <td align="center">{$item.BankAccount|default:'&nbsp;'}</span></td>                <td align="center">{$item.SalaryValue}</span></td>                <td align="center">{$item.FullName}</span></td>                <td align="center">{$month}</span></td>                <td align="center">{if $smarty.get.SalaryPaymentType == 1}AVANS{else}LICHIDARE{/if}</span></td>            </tr>        {/foreach}    {else}        <tr height="30">            <td colspan="{if !empty($smarty.get.export)}18{else}19{/if}" class="celulaMenuSTDR">{translate label='Nu sunt date'}!</td>        </tr>    {/if}</table>